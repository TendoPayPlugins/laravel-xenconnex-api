{
  "name": "tendopay/laravel-saltedge-api",
  "description": "SaltEdge API Integration",
  "type": "library",
  "authors": [
    {
      "name": "TendoPay",
      "email": "contact@tendopay.ph",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": "6.x",
    "guzzlehttp/guzzle": "^7.4.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.18",
    "orchestra/testbench": "^v4.18.0"
  },
  "autoload": {
    "psr-4": {
      "TendoPay\\Integration\\XenConnex\\": "src/TendoPay/Integration/XenConnex"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TendoPay\\Integration\\XenConnex\\": "test/TendoPay/Integration/XenConnex"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TendoPay\\Integration\\XenConnex\\XenConnexServiceProvider"
      ]
    }
  }
}
